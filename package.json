  "dependencies": {
    "@types/express": "^4.17.13",
    "cds-internal-tool": "^1.2.0",
    "espree": "^9.3.2"
  },
  "devDependencies": {
    "@types/jest": "28.1.4",
    "@types/node": "16.11.42",
    "@typescript-eslint/eslint-plugin": "5.30.0",
    "@typescript-eslint/parser": "5.30.0",
    "eslint": "8.18.0",
    "eslint-plugin-jest": "26.5.3",
    "jest": "28.1.2",
    "rimraf": "3.0.2",
    "ts-jest": "28.0.5",
    "ts-node": "10.8.1",
    "typescript": "4.7.4"
  },
  "optionalDependencies": {
    "fsevents": "^2.3.2"
  },
  "keywords": [
    "CAP",
    "cds",
