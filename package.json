    "@typescript-eslint/parser": "5.38.1",
    "eslint": "8.24.0",
    "eslint-plugin-jest": "27.0.4",
    "jest": "29.0.3",
    "ts-jest": "29.0.2",
    "ts-node": "10.9.1",
    "typescript": "4.8.4"
  },
  "optionalDependencies": {
    "fsevents": "*"
