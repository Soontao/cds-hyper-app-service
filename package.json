  "main": "lib/index.js",
  "description": "CAP Hyper ApplicationService, for extension",
  "engines": {
    "node": ">=14",
    "npm": ">=6"
  },
  "author": {
    "name": "Theo Sun"
  },
  "scripts": {
    "test": "jest --detectOpenHandles --passWithNoTests",
