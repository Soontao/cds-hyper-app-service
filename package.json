{
  "name": "cds-hyper-impl",
  "version": "0.0.1",
  "main": "lib/index.js",
  "description": "enhanced @impl annotation for CAP nodejs runtime",
  "engines": {
    "node": ">=12",
    "npm": ">=6"
  },
  "author": {
    "name": "Theo Sun"
  },
  "scripts": {
    "test": "jest --detectOpenHandles --passWithNoTests",
    "install:dev": "bash install_no_trace_deps.sh",
    "pretest": "npm run install:dev",
    "prelint": "npm run install:dev",
    "prebuild": "rimraf lib",
    "build": "tsc",
    "postbuild": "tsc --noEmit --p tsconfig.lib.json",
    "lint": "eslint src/** --fix",
    "release": "npx standard-version --no-verify",
    "coverage": "npm test -- --coverage --no-cache"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^16.2.1",
    "@types/jest": "^27.4.1",
    "@types/node": "^16.11.29",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "cds-internal-tool": "^0.0.4",
    "commitlint": "^16.2.3",
    "eslint": "^8.14.0",
    "eslint-plugin-jest": "^26.1.5",
    "husky": "~4.3.8",
    "jest": "^27.5.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "keywords": [
    "CAP",
    "cds",
    "service"
  ],
  "license": "Apache-2.0"
}
